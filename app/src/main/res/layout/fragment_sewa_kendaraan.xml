<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SewaKendaraan.SewaKendaraanFragment">

    <ImageView
        android:id="@+id/imgMobil"
        android:layout_width="169dp"
        android:layout_height="169dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="236dp"
        android:src="@drawable/icon_mobil"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgMotor"
        android:layout_width="158dp"
        android:layout_height="170dp"
        android:layout_marginStart="32dp"
        android:src="@drawable/icon_motor"
        app:layout_constraintStart_toEndOf="@+id/imgMobil"
        app:layout_constraintTop_toTopOf="@+id/imgMobil" />

    <TextView
        android:id="@+id/textView5"
        style="@style/TextAppearance.AppCompat.Display1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="96dp"
        android:text="Pilih Kendaraan Yang ingin disewa"
        android:textSize="10pt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:text="Mobil"
        android:textSize="10pt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgMobil" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="88dp"
        android:text="Motor"
        android:textSize="10pt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgMotor" />


</androidx.constraintlayout.widget.ConstraintLayout>